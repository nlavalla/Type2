<?xml version="1.0" encoding="utf-8"?>

<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:tools="http://schemas.android.com/tools"
android:layout_width="match_parent"
android:layout_height="match_parent"
tools:context=".MainActivity">
<fragment
    android:id="@+id/arFragment"
    android:name="com.example.type2_01_05.CustomArFragment"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    />
    <ImageView
        android:id="@+id/image_view_fit_to_scan"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_gravity="center"
        android:scaleType="fitCenter"
        android:src="@drawable/fit_to_scan"
        android:contentDescription="@string/fit_image_to_scan"
        />
    <Button
        android:id="@+id/button"
        android:layout_width="88dp"
        android:layout_height="94dp"
        android:layout_gravity="right"
        android:background="@drawable/nextbuttonn"
        android:backgroundTintMode="src_atop"
        android:text="@string/button" />

</FrameLayout>